    "label": "spring,configuration",
    "deprecated": "false",
    "input": "false",
    "output": "false"
  },
  "properties": {
    "poolSize": { "kind": "attribute", "required": "true", "type": "string", "javaType": "java.lang.String", "deprecated": "false", "secret": "false", "description": "Sets the core pool size (threads to keep minimum in pool)" },
    "maxPoolSize": { "kind": "attribute", "required": "false", "type": "string", "javaType": "java.lang.String", "deprecated": "false", "secret": "false", "description": "Sets the maximum pool size" },
