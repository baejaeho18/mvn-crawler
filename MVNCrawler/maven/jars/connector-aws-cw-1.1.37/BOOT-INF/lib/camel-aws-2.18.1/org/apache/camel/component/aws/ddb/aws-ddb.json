    "label": "cloud,database,nosql",
    "deprecated": "false",
    "async": "false",
    "producerOnly": "true",
    "javaType": "org.apache.camel.component.aws.ddb.DdbComponent",
    "groupId": "org.apache.camel",
    "artifactId": "camel-aws",
    "version": "2.18.1"
  },
  "componentProperties": {
  },
