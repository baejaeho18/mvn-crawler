{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "description": "A BoostDiagnostic following the convention of http://microformats.org/wiki/hcard",
    "type": "object",
    "properties": {
    	"connectionId": { "type": "string" },
        "node": { "$ref": "../NodeAddress.json#/definitions/nodeAddress" },
        "current": { "type": "integer" },
        "maxCurrent": { "type": "integer" },
        "voltage": { "type": "integer" },
        "temperature": { "type": "integer" }
    },
    "required": ["connectionId", "node"]
}