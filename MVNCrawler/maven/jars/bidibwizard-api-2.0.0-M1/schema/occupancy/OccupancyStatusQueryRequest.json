{
    "$schema": "http://json-schema.org/draft-06/schema#",
	"description": "Query the occupancy port status of the node in the bidib system.",
    "type": "object",
    "properties": {
        "connectionId": {
            "type": "string"
        },
        "node": {
            "$ref": "../NodeAddress.json#/definitions/nodeAddress"
        },
        "portRangeStart": {
            "type": "integer"
        },
        "portRangeEnd": {
            "type": "integer"
        }
    },
    "required": ["connectionId", "node"]
}