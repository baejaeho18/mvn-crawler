{
    "$schema": "http://json-schema.org/draft-06/schema#",
	"definitions": {
		"nodeAddress": {
		    "description": "A NodeAddress following the convention of http://microformats.org/wiki/hcard",
		    "type": "object",
		    "properties": {
		        "address": { "type": "string" },
		        "uniqueId": { "type":"string" }
		    },
		    "required": ["address", "uniqueId"]
		}
	}
}
