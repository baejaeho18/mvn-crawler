{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "definitions": {
        "switchPortStatus": {
            "description": "A SwitchPort Status following the convention of http://microformats.org/wiki/hcard",
            "type": "string",
            "enum": [
                "ON",
                "OFF",
                "UNKNOWN",
                "TEST"
            ],
            "javaEnumNames": [
                "ON",
                "OFF",
                "UNKNOWN",
                "TEST"
            ]
        },
        "soundPortStatus": {
            "description": "A SoundPort Status following the convention of http://microformats.org/wiki/hcard",
            "type": "string",
            "enum": [
                "TURN_ON",
                "TURN_OFF",
                "UNKNOWN",
                "TEST"
            ],
            "javaEnumNames": [
                "PLAY",
                "STOP",
                "UNKNOWN",
                "TEST"
            ]
        }
    }
}