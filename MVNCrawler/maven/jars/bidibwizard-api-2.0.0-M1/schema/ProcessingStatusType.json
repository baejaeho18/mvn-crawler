{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "description": "A ProcessingStatusType following the convention of http://microformats.org/wiki/hcard",
    "type": "object",
    "properties": {
        "nodeId": { "type": "number" },
        "statusLevel" : { "$ref": "StatusLevel.json#/definitions/statusLevel" },
        "msgKey": { "type": "String" },
        "msgKeyArgs" : { 
        	"type": "array"
        }
    },
    "required": ["nodeId", "msgKey"]
}