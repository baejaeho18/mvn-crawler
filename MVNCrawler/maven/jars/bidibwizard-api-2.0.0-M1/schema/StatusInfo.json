{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "description": "A StatusInfo following the convention of http://microformats.org/wiki/hcard",
    "type": "object",
    "properties": {
        "connectionId": { "type": "string" },
 		"statusLevel" : { "$ref": "StatusLevel.json#/definitions/statusLevel" },
        "msgKey": { "type": "string" },
        "msgKeyArgs" : { 
        	"type" : "array"
        },
        "duration": { "type": "integer" }
    },
    "required": ["connectionId", "messageKey"]
}