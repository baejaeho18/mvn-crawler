{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"definitions": {
		"versionType": {
		    "description": "A Version following the convention of http://microformats.org/wiki/hcard",
		    "type": "object",
		    "properties": {
		        "version": { "type": "string" },
		        "pid": { "type": "integer" },
		        "vendor" : { "type": "integer" }
		    },
		    "required": ["version", "pid", "vendor"]
    	}
  	}
}