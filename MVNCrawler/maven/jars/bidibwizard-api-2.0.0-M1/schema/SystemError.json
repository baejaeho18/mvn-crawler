{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "description": "A SystemError following the convention of http://microformats.org/wiki/hcard",
    "type": "object",
    "properties": {
        "msgKey": { "type": "string" },
        "msgKeyArgs" : { 
        	"type" : "array"
        },
        "timestamp": { "type": "string", "format": "date-time" },
        "connectionId": { "type": "string" }
    },
    "required": ["messageKey", "timestamp"]
}