
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<artifactId>restcomm-slee-ra-mscontrol-docs</artifactId>
		<groupId>org.mobicents.resources.mscontrol</groupId>
		<version>2.8.53</version>
	</parent>

	<artifactId>restcomm-slee-ra-mscontrol-docs-sources-telscale</artifactId>

	<properties>
		<THIS.PLATFORM>Telscale</THIS.PLATFORM>
		<JEE.PLATFORM>JBoss Enterprise Application Platform</JEE.PLATFORM>
		<THIS.ISSUE_TRACKER_URL>https://telestax.zendesk.com/home</THIS.ISSUE_TRACKER_URL>
		<THIS.PACKAGE_NAME>media</THIS.PACKAGE_NAME>
		<THIS.SOURCE_CODE_URL>NOT AVAILABLE</THIS.SOURCE_CODE_URL>
		<author.email.eduardo>emartins (at) redhat.com</author.email.eduardo>
		<author.email.bartosz>bbaranow (at) redhat.com</author.email.bartosz>
		<author.email.alex>amendonca (at) redhat.com</author.email.alex>
	</properties>

	<build>
		<resources>
			<resource>
				<directory>${basedir}/src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>unpack</id>
						<phase>generate-resources</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>${pom.groupId}</groupId>
									<artifactId>restcomm-slee-ra-mscontrol-docs-sources</artifactId>
									<version>${pom.version}</version>
									<type>jar</type>
									<overWrite>true</overWrite>
									<outputDirectory>${basedir}/src/main/resources</outputDirectory>
								</artifactItem>
							</artifactItems>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>clean-resources</id>
						<phase>clean</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<tasks>
								<delete failonerror="false" includeemptydirs="true">
									<fileset dir="${basedir}/src" />
								</delete>
							</tasks>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

</project>
