  "properties": [
    {
      "name": "authz.app",
      "type": "java.lang.String",
      "description": "@since 1.1.0",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties",
      "defaultValue": "defaultApp"
    },
    {
      "name": "authz.banner",
      "type": "java.lang.Boolean",
      "description": "@since 1.1.2",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties",
      "defaultValue": true
    },
    {
      "name": "authz.cache.cache-maximum-size",
      "type": "java.lang.Long",
      "description": "最大缓存数，不配置时默认无大小限制",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$CacheConfig"
    },
    {
      "name": "authz.cache.enable-redis",
      "type": "java.lang.Boolean",
      "description": "是否开启redis缓存（两级缓存） L2",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$CacheConfig",
      "defaultValue": false
    },
    {
      "name": "authz.cache.enable-redis-actuator",
      "type": "java.lang.Boolean",
      "description": "是否开启redis健康监控检查，默认关闭",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$CacheConfig",
      "defaultValue": false
    },
    {
      "name": "authz.cache.expire-after-read-or-update-time",
      "type": "java.lang.String",
      "description": "在读取或者更新之后刷新缓存存活的时间 默认10分钟",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$CacheConfig",
      "defaultValue": "10m"
    },
    {
      "name": "authz.cache.redis-scan-count",
      "type": "java.lang.Integer",
      "description": "redis每次扫描key的数量",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$CacheConfig",
      "defaultValue": 10000
    },
    {
      "name": "authz.dashboard.allow",
      "type": "java.lang.String",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$DashboardConfig"
    },
    {
      "name": "authz.dashboard.deny",
      "type": "java.lang.String",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$DashboardConfig"
    },
    {
      "name": "authz.dashboard.enabled",
      "type": "java.lang.Boolean",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$DashboardConfig",
      "defaultValue": false
    },
    {
      "name": "authz.dashboard.mappings",
      "type": "java.lang.String",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$DashboardConfig",
      "defaultValue": "\/authz-dashboard\/*"
    },
    {
      "name": "authz.dashboard.password",
      "type": "java.lang.String",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$DashboardConfig"
    },
    {
      "name": "authz.dashboard.remote-address",
      "type": "java.lang.String",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$DashboardConfig"
    },
    {
      "name": "authz.dashboard.username",
      "type": "java.lang.String",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$DashboardConfig"
    },
    {
      "name": "authz.default-decryptor",
      "type": "java.lang.Class<? extends cn.omisheep.authz.core.codec.Decryptor>",
      "description": "@since 1.0.11",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties"
    },
    {
      "name": "authz.gc-period",
      "type": "java.lang.String",
      "description": "定期GC时间，单位 s|m|h|d 为0则关闭",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties"
    },
    {
      "name": "authz.global-ip-range.allow",
      "type": "java.lang.String",
      "description": "若配置，则必须在这些范围内 xx.xx.xx.xx\/xx , xx.xx.xx.xx\/xx",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$IpRangeConfig",
      "defaultValue": ""
    },
    {
      "name": "authz.global-ip-range.deny",
      "type": "java.lang.String",
      "description": "拒绝访问网断 xx.xx.xx.xx\/xx , xx.xx.xx.xx\/xx",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$IpRangeConfig",
      "defaultValue": ""
    },
    {
      "name": "authz.global-ip-range.support-native",
      "type": "java.lang.Boolean",
      "description": "0:0:0:0:0:0:0:1  127.0.0.1是否支持",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$IpRangeConfig",
      "defaultValue": true
    },
    {
      "name": "authz.ignore-suffix",
      "type": "java.lang.String[]",
      "description": "过滤后缀名",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties",
      "defaultValue": [
        ".css",
        ".js",
        ".html",
        ".png",
        ".jpg",
        ".gif",
        ".svg"
      ]
    },
    {
      "name": "authz.log",
      "type": "org.springframework.boot.logging.LogLevel",
      "description": "authz的日志等级",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties"
    },
    {
      "name": "authz.md5check",
      "type": "java.lang.Boolean",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties",
      "defaultValue": false
    },
    {
      "name": "authz.orm",
      "type": "cn.omisheep.authz.core.AuthzProperties$ORM",
      "description": "orm框架 目前仅支持mybatis @since 1.0.5",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties"
    },
    {
      "name": "authz.response.always-ok",
      "type": "java.lang.Boolean",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$ResponseConfig",
      "defaultValue": false
    },
    {
      "name": "authz.rsa.auto",
      "type": "java.lang.Boolean",
      "description": "是否开启自动刷新RSA",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$RSAConfig",
      "defaultValue": true
    },
    {
      "name": "authz.rsa.custom-private-key",
      "type": "java.lang.String",
      "description": "自定义私钥",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$RSAConfig"
    },
    {
      "name": "authz.rsa.custom-public-key",
      "type": "java.lang.String",
      "description": "自定义公钥",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$RSAConfig"
    },
    {
      "name": "authz.rsa.rsa-key-refresh-with-period",
      "type": "java.lang.String",
      "description": "auto为true时生效 rsa的key刷新时间，单位 s|m|h|d",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$RSAConfig",
      "defaultValue": "7d"
    },
    {
      "name": "authz.token.cookie-name",
      "type": "java.lang.String",
      "description": "cookie name",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$TokenConfig",
      "defaultValue": "atkn"
    },
    {
      "name": "authz.token.header-name",
      "type": "java.lang.String",
      "description": "header name",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$TokenConfig",
      "defaultValue": "Authorization"
    },
    {
      "name": "authz.token.header-prefix",
      "type": "java.lang.String",
      "description": "prefix 例如：\"Bearer <token>\"",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$TokenConfig",
      "defaultValue": "Bearer"
    },
    {
      "name": "authz.token.issuer",
      "type": "java.lang.String",
      "description": "issuer",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$TokenConfig",
      "defaultValue": "au"
    },
    {
      "name": "authz.token.key",
      "type": "java.lang.String",
      "description": "签名的私钥",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$TokenConfig"
    },
    {
      "name": "authz.token.live-time",
      "type": "java.lang.String",
      "description": "存活时间，默认 7d ，单位 ms|s|m|h|d",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$TokenConfig",
      "defaultValue": "7d"
    },
    {
      "name": "authz.token.refresh-time",
      "type": "java.lang.String",
      "description": "刷新时间，默认 30d ，单位 ms|s|m|h|d",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$TokenConfig",
      "defaultValue": "30d"
    },
    {
      "name": "authz.user-buffer-refresh-with-period",
      "type": "java.lang.String",
      "description": "开启redis缓存时可以不用设置。用户缓存刷新频率，清除掉过期掉值 默认10秒一次，单位 s|m|h|d",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties",
      "defaultValue": "10s"
    },
    {
      "name": "authz.user.support-multi-device",
      "type": "java.lang.Boolean",
      "description": "是否支持多设备登录(type不同，id不同)",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$UserConfig",
      "defaultValue": true
    },
    {
      "name": "authz.user.support-multi-user-for-same-device-type",
      "type": "java.lang.Boolean",
      "description": "是否支持同类型设备多登录（type相同，id不同）",
      "sourceType": "cn.omisheep.authz.core.AuthzProperties$UserConfig",
      "defaultValue": false
    }
  ],
  "hints": []
}