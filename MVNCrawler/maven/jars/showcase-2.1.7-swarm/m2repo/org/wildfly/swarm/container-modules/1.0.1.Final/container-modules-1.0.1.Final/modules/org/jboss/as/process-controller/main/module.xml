<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<module xmlns="urn:jboss:module:1.3" name="org.jboss.as.process-controller">
<properties>
    <property name="jboss.api" value="private"/>
    <property name="jboss.require-java-version" value="1.8"/>
  </properties>
  <exports>
    <exclude path="org/jboss/as/process/logging"/>
  </exports>
  <main-class name="org.jboss.as.process.Main"/>
  <resources>
    <artifact name="org.wildfly.core:wildfly-process-controller:2.0.10.Final"/>
  </resources>
  <dependencies>
    <module name="javax.api"/>
    <module name="org.wildfly.security.elytron" services="import"/>
    <module export="true" name="org.jboss.as.version"/>
    <module name="org.jboss.logging"/>
    <module name="org.jboss.logmanager" services="import"/>
    <module name="org.jboss.marshalling"/>
    <module name="org.jboss.marshalling.river" services="import"/>
    <module name="org.jboss.modules"/>
    <module name="org.jboss.threads"/>
  </dependencies>
</module>
