<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<module xmlns="urn:jboss:module:1.3" name="org.jboss.as.webservices.server.integration">
<properties>
    <property name="jboss.api" value="private"/>
  </properties>
  <resources>
    </resources>
  <dependencies>
    <module export="true" name="javax.api"/>
    <module export="true" name="javax.jws.api"/>
    <module export="true" name="javax.wsdl4j.api"/>
    <module export="true" name="javax.xml.ws.api"/>
    <module export="true" name="com.sun.xml.bind" services="export"/>
    <module export="true" name="org.jboss.ws.api"/>
    <module export="true" name="org.jboss.ws.spi"/>
    <module export="true" name="org.jboss.ws.common" services="import"/>
    <module export="true" name="org.jboss.ws.jaxws-client" services="export">
        <imports>
          <include path="META-INF/cxf"/>
        <include path="META-INF"/>
      </imports>
      <exports>
          <include path="META-INF/cxf"/>
        <include path="META-INF"/>
      </exports>
    </module>
    <module export="true" name="org.jboss.ws.cxf.jbossws-cxf-factories" services="export"/>
    <module export="true" name="org.jboss.ws.cxf.jbossws-cxf-transports-undertow">
        <imports>
          <include path="META-INF/cxf"/>
        <include path="META-INF"/>
      </imports>
      <exports>
          <include path="META-INF/cxf"/>
        <include path="META-INF"/>
      </exports>
    </module>
    <module export="true" name="org.jboss.ws.cxf.jbossws-cxf-transports-udp">
        <imports>
          <include path="META-INF/cxf"/>
        <include path="META-INF"/>
      </imports>
      <exports>
          <include path="META-INF/cxf"/>
        <include path="META-INF"/>
      </exports>
    </module>
    <module export="true" name="org.jboss.ws.cxf.jbossws-cxf-server" services="export"/>
    <!-- Do not import services from cxf module directly, those need to come from jbossws -->
    <module export="true" name="org.apache.cxf.impl">
        <imports>
          <include path="META-INF/cxf"/>
        <include path="META-INF"/>
      </imports>
      <exports>
          <include path="META-INF/cxf"/>
        <include path="META-INF"/>
      </exports>
    </module>
    <module export="true" name="org.apache.xalan" services="export"/>
    <module export="true" name="org.apache.xerces" services="export"/>
    <module export="true" name="org.jboss.as.webservices" services="export"/>
    <module export="true" name="com.sun.xml.messaging.saaj" services="export"/>
    <module export="true" name="org.apache.ws.security"/>
    <module export="true" name="org.apache.santuario.xmlsec"/>
    <module export="true" name="org.bouncycastle"/>
    <module name="org.jboss.xts">
        <imports>
          <include path="com.arjuna.mw.wst11.client"/>
        <include path="org.jboss.jbossts.txbridge.outbound"/>
      </imports>
    </module>
  </dependencies>
</module>
