<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<module xmlns="urn:jboss:module:1.3" name="org.jboss.as.domain-management">
<properties>
    <property name="jboss.api" value="private"/>
  </properties>
  <exports>
    <exclude path="org/jboss/as/domain/management/logging"/>
    <exclude path="org/jboss/as/domain/management/_private"/>
  </exports>
  <resources>
    <artifact name="org.wildfly.core:wildfly-domain-management:2.0.10.Final"/>
  </resources>
  <dependencies>        
    <module name="org.wildfly.security.elytron"/>
    <module name="org.jboss.as.controller"/>
    <module name="org.jboss.as.core-security"/>
    <module name="org.jboss.logging"/>
    <module name="org.jboss.logmanager"/>
    <module name="org.jboss.modules"/>
    <module name="org.jboss.msc"/>
    <module name="org.jboss.sasl"/>
    <module name="org.jboss.staxmapper"/>
    <module name="org.picketbox" optional="true"/>
    <module name="javax.api"/>
    <module export="true" name="sun.jdk"/>
  </dependencies>
</module>
