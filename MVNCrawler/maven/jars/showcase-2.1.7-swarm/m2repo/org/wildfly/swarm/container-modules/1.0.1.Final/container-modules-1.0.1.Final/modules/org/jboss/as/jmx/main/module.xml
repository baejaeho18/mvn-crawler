<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<module xmlns="urn:jboss:module:1.3" name="org.jboss.as.jmx">
<properties>
    <property name="jboss.api" value="private"/>
  </properties>
  <exports>
    <exclude path="org/jboss/as/jmx/logging"/>
  </exports>
  <resources>
    <artifact name="org.wildfly.core:wildfly-jmx:2.0.10.Final"/>
  </resources>
  <dependencies>
    <module name="javax.api"/>
    <module name="org.jboss.staxmapper"/>
    <module name="org.jboss.as.controller"/>
    <module name="org.jboss.as.core-security"/>
    <module name="org.jboss.as.domain-management"/>
    <module name="org.jboss.as.network"/>
    <module name="org.jboss.as.remoting"/>
    <module name="org.wildfly.security.elytron"/>
    <module name="org.jboss.as.server"/>
    <module name="org.jboss.remoting"/>
    <module name="org.jboss.msc"/>
    <module name="org.jboss.logging"/>
    <module name="org.jboss.remoting-jmx" optional="true" services="import"/>
  </dependencies>
</module>
